{
  "name": "console-debug",
  "version": "1.0.0",
  "description": "Replaces the console object with a more stylish and practical way of displaying messages.",
  "main": "main.js",
  "scripts": {
    "lint": "node ./node_modules/eslint/bin/eslint.js --config ./.eslintrc --ext .js src ./**/*.js",
    "test": "mocha",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec ./test/*"
