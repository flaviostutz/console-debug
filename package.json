    }
  ],
  "license": "MIT",
  "devDependencies": {
    "assert-plus": "^1.0.0",
    "coveralls": "^2.11.15",
    "eslint": "^3.10.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "mocha-lcov-reporter": "^1.2.0",
    "istanbul": "^0.4.5",
    "package-script": "^0.0.8",
    "pre-commit": "^1.1.3"
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
